{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": " dotenv-run-script .env.development -- env-dev",
    "env-dev": "kill-port $PORT ;  npm run css-watch & nuxt dev --port $PORT ; fg",
    "build": "nuxt build",
    "css-build": "sass --no-source-map assets/scss/main.scss assets/css/main.css > ../log/sass.log",
    "css-watch": "npm run css-build -- --watch",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.2.1",
    "dotenv-run-script": "^0.2.0",
    "kill-port": "^2.0.0",
    "sass": "^1.52.1",
    "sass-loader": "10.1.1",
    "webpack": "^4.46.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "axios": "^0.27.2",
    "bootstrap-icons": "^1.8.3",
    "bulma": "^0.9.4",
    "bulma-tooltip": "^3.0.2",
    "core-js": "^3.19.3",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "nuxt": "^2.15.8",
    "tiny-cookie": "^2.3.2",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  }
}

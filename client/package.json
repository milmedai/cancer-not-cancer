{
  "name": "client",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev --dotenv .env.development",
    "old-dev": "kill-port $PORT ;  npm run css-watch & nuxt --dotenv .env.development --port $PORT ; fg",
    "build": "NODE_OPTIONS=--openssl-legacy-provider nuxt build --dotenv .env.production",
    "generate": "NODE_OPTIONS=--openssl-legacy-provider nuxt generate --dotenv .env.production",
    "buildDev": "nuxt build --dotenv .env.development",
    "generateDev": "nuxt generate --dotenv .env.development",
    "css-build": "sass --no-source-map assets/scss/main.scss assets/css/main.css > ../log/sass.log",
    "css-watch": "npm run css-build -- --watch",
    "staging": "NODE_OPTIONS=--openssl-legacy-provider nuxt start --dotenv .env.staging --port $PORT",
    "build-stage": "NODE_OPTIONS=--openssl-legacy-provider nuxt build --dotenv .env.staging",
    "gen-stage": "NODE_OPTIONS=--openssl-legacy-provider nuxt generate --dotenv .env.staging"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@pinia/nuxt": "^0.5.1",
    "bulma": "^0.9.4",
    "form-data": "^4.0.0",
    "kill-port": "^2.0.0",
    "nuxt": "^3.8.0",
    "pinia": "^2.1.7",
    "sass": "^1.52.1",
    "vue": "^3.3.7",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.2.5"
  }
}
